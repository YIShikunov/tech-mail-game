<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
	<div class="title">WuXing</div>
	<div class="score">
		<div class="score__title">Рейтинг</div>
		<table class="score__table">
				<tr class="score__tr">
					<td class="score__td_name">Name</td>
					<td class="score__td_score">Score</td>
				</tr>
			<fest:each iterate="json" index="i" value="score">
				<tr class="score__tr">
					<td class="score__td_name"><fest:value>score.name</fest:value></td>
					<td class="score__td_score"><fest:value>score.score</fest:value></td>
				</tr>
			</fest:each>
		</table>
		<a class="score__btn" href="#">НАЗАД</a>
	</div>
</fest:template>
